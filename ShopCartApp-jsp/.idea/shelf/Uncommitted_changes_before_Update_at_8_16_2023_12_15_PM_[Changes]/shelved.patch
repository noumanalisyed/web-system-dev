+	<link type="text/css" rel="stylesheet" href="css/add-student-style.css">
+</head>
+
+<body>
+	<div id="wrapper">
+		<div id="header">
+			<h2>Course Registration App</h2>
+			<c:set var="context" value="${pageContext.request.contextPath}" />
+		</div>
+	</div>
+	
+	<div id="container">
+		<h3>Update Registration</h3>
+		
